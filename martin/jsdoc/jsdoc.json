{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./js/"],
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "readme": "README.md",
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "static": "true",
    "sort": "false",
    "meta": {
      "title": "webapps",
      "description": "Opdracht voor Webapplicaties voor de OU",
      "keyword": "Webapplicaties Clientkant OU"
    },
    "menu": {
      "Project Website": {
        "href":"http://localhost:80",
        "target":"_blank",
        "class":"menu-item",
        "id":"website_link"
      },
      "Tests": {
        "href":"http://localhost:80/test",
        "target":"_blank",
        "class":"menu-item",
        "id":"forum_link"
      }
    }
  }
}